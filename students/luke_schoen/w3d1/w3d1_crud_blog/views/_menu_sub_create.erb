<%# content_for :menu_sub do %>
		
		<div id="return_home">
            <a href="/blank">Hide</a>
        </div>

		<div id="display_titles">
        	<h2>Create Post:</h2><br>
        	<%# modify data in database. shows the form and click create will go here to create database. anything with 'name' will be recognised by sinatra %>

			<form method='post' action="/_menu_sub_create"> <%# posts to main.rb post '/create' %>
				<label for="">Title:</label>
				<input type="text" name="title"> 
				<label for="">Abstract:</label>
				<input type="text" name="abstract">
				<label>Body Text:</label>
				<input type="text" name="body_text">
				<label>Author:</label>
				<input type="text" name="author">
				<button>Create New Post</button>
			</form>

        </div>

<%# end %>